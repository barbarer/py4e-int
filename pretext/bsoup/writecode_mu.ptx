          <cline>    rows = tag.find_all('tr')</cline>
        </choice>
        <choice>
          <cline>    rows = table.find_all('td') </cline>
        </choice>
      </block>
      <block order="9">
        <cline>    for row in rows[1:]:</cline>
      </block>
